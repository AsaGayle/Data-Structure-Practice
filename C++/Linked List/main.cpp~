#include <iostream>
#include <vector>
#include <fstream>

using namespace std;

class Node{
	private:
		int data;
	public: 
		Node* next;
		int getData(){
			return data;
		}
		int setData(int nData){
			data = nData;
		}
	Node(int nData);
};	

class LinkedList{
	private:
	
	public:
		Node* head = NULL;
		Node* current = new Node;
		current = head;
		void loadVector(vector<int>& arr);
};

void LinkedList::loadVector(vector<int>& arr){
	for(int i=0; i<arr.size(); i++){
		current.setData(arr[i]);
		current.next = new Node;
		current = current.next;
	}
}
int main(){
	Node testNode;
	Node* current = &testNode;

	return 0;
}
